:root{--monokai-bg:#272822;--monokai-fg:#f8f8f2;--monokai-grey:#928e7a;--monokai-red:#F92672;--monokai-orange:#FD971F;--monokai-light-orange:#E69F66;--monokai-yellow:#FFD866;--monokai-green:#A6E22E;--monokai-blue:#66D9EF;--monokai-purple:#AE81FF}@media screen and (max-width:1023px){.sticky-toc{display:none}}.sticky-toc .toc-title{font-size:1.25em;font-weight:700}.sticky-toc .toc-title:before{content:"<";color:var(--monokai-yellow)}.sticky-toc .toc-title:after{content:">";color:var(--monokai-yellow)}.sticky-toc .toc-content{font-size:1em}.sticky-toc .toc-content ul{font-size:90%;padding-left:20px;margin:5px 0}.sticky-toc .toc-content ul ul ul ul{font-size:100%}.sticky-toc .toc-content li::marker{content:"# ";color:var(--monokai-yellow);font-size:inherit}.post-metadata{color:var(--monokai-grey)!important;font-size:80%}.post-article *::before,.post-article *::after{display:inline-block;margin:5px;text-decoration:none;font-weight:bolder;color:var(--monokai-yellow)}.post-article .post-metadata{margin-left:30px;margin-bottom:20px}.post-article h1{margin-left:10px}.post-article h1::before{content:"<"}.post-article h1::after{content:">"}.post-article .post-article-description{margin-left:30px;font-style:italic}.post-article hr{border:var(--monokai-grey)solid 1px;margin:10px 20px}.post-article .post-article-content h2{margin-left:10px}.post-article .post-article-content h3,.post-article .post-article-content h4{margin-left:20px}.post-article .post-article-content h5,.post-article .post-article-content h6,.post-article .post-article-content p,.post-article .post-article-content ul,.post-article .post-article-content ol,.post-article .post-article-content code{margin-left:30px}.post-article .post-article-content h2::before,.post-article .post-article-content h3::before,.post-article .post-article-content h4::before{content:"#";font-size:90%}.post-article .post-article-content code{margin:0}.post-article .post-article-content img{margin:0 auto}.post-article .post-article-content blockquote{border:none;margin:0}.post-article .post-article-content blockquote p::before,.post-article .post-article-content blockquote p::after{display:block;margin:0 5%;font-size:200%}.post-article .post-article-content blockquote p::after{text-align:right}.post-article .post-article-content table,.post-article .post-article-content th,.post-article .post-article-content td{border:1px solid var(--monokai-fg);border-collapse:collapse}.post-article .post-article-content table{margin:0 40px;width:auto}.post-article .post-article-content th,.post-article .post-article-content td{padding:8px}.post-article .post-article-content .footnotes li p{margin-left:0}.post-article .post-article-content .spoiler{border:2px solid var(--monokai-grey);border-radius:10px;box-shadow:0 0 3px 1px var(--monokai-grey);margin:10px;height:auto}.post-article .post-article-content .spoiler *{transition:all .2s ease-in-out}.post-article .post-article-content .spoiler .spoiler_control{margin:.5em 1em;color:var(--monokai-fg);width:100%;text-align:left}.post-article .post-article-content .spoiler .spoiler_control:hover{cursor:pointer}.post-article .post-article-content .spoiler .spoiler_control span{vertical-align:top;transform:rotate(0);display:inline-block;margin-right:1em;color:var(--monokai-yellow)}.post-article .post-article-content .spoiler .spoiler_content{overflow:hidden;max-height:0}.post-article .post-article-content .spoiler.visible .spoiler_control{color:var(--monokai-blue)}.post-article .post-article-content .spoiler.visible .spoiler_control span{color:inherit;transform:rotate(.5turn)}.post-tags{margin-left:30px;font-size:80%}.post-tags span{color:var(--monokai-grey);margin:0 10px}